{
  "name": "public",
  "version": "1.0.0",
  "description": "A sophisticated AI-powered project roadmap and management application built with modern web technologies.",
  "main": "index.js",
  "scripts": {
    "build-css": "npx tailwindcss -i src/styles.css -o dist/styles.css",
    "build-css:watch": "npx tailwindcss -i src/styles.css -o dist/styles.css --watch",
    "build": "npm run build-css && node scripts/build.js",
    "dev": "concurrently \"npm run build-css:watch\" \"live-server --host=localhost --port=8000 --open=/dist/\"",
    "start": "npm run build && live-server --host=localhost --port=8000 --open=/dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hokage-123/portfolio-command-center.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Hokage-123/portfolio-command-center/issues"
  },
  "homepage": "https://github.com/Hokage-123/portfolio-command-center#readme",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "live-server": "^1.2.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.17"
  }
}
